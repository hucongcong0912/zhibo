window.pushReponseFunctions.liveNotify=function(b){var a=DUYA_PUSH.mapSubscribeList(b,DUYA_PUSH.userSubscribeList);DUYA_PUSH.refreshPush(a)};window.pushReponseFunctions.unreadMsgNotify=function(a){DUYA_INS.myMessageShow();setTimeout(DUYA_INS.myMessageHide,1000*300)};window.pushReponseFunctions.matchNotice=function(a){if(typeof a!="undefined"&&$.isArray(a)&&typeof(updataMatchNotices)=="function"){matchNoticeData=a;updataMatchNotices(a)}};window.pushReponseFunctions.refreshMatchNoticeCron=function(){setInterval(function(){if(typeof matchNoticeData!="undefined"&&typeof(updataMatchNotices)=="function"){updataMatchNotices(matchNoticeData)}},300000)};PushService.pushOpen=function(a){if(window.currentUid!=""&&typeof window.currentUid!="undefined"&&window.currentUid>0){DUYA_INS.userId=window.currentUid;a.sendMessage('{"event":"login","pushData":{"uid":'+window.currentUid+"}}")}if(typeof G_info!="undefined"&&typeof G_info.mid!="undefined"&&G_info.mid>0&&G_info.mid!=62){var b={event:"joinRoom",pushData:{type:4,data:["match_room_"+G_info.mid]}};a.sendMessage(JSON.stringify(b));window.pushReponseFunctions.refreshMatchNoticeCron()}};$(window).on("load",function(){PushService.init()});
